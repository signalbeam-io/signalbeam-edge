{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:8080"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "devices-route": {
        "ClusterId": "device-manager",
        "Match": {
          "Path": "/api/devices/{**catch-all}"
        }
      },
      "groups-route": {
        "ClusterId": "device-manager",
        "Match": {
          "Path": "/api/groups/{**catch-all}"
        }
      },
      "registration-tokens-route": {
        "ClusterId": "device-manager",
        "Match": {
          "Path": "/api/registration-tokens/{**catch-all}"
        }
      },
      "bundles-route": {
        "ClusterId": "bundle-orchestrator",
        "Match": {
          "Path": "/api/bundles/{**catch-all}"
        }
      },
      "rollouts-route": {
        "ClusterId": "bundle-orchestrator",
        "Match": {
          "Path": "/api/rollouts/{**catch-all}"
        }
      },
      "phased-rollouts-route": {
        "ClusterId": "bundle-orchestrator",
        "Match": {
          "Path": "/api/phased-rollouts/{**catch-all}"
        }
      },
      "heartbeat-route": {
        "ClusterId": "telemetry-processor",
        "Match": {
          "Path": "/api/heartbeat/{**catch-all}"
        }
      },
      "auth-route": {
        "ClusterId": "identity-manager",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        }
      },
      "subscriptions-route": {
        "ClusterId": "identity-manager",
        "Match": {
          "Path": "/api/subscriptions/{**catch-all}"
        }
      },
      "oidc-wellknown-route": {
        "ClusterId": "zitadel",
        "Match": {
          "Path": "/.well-known/{**catch-all}"
        }
      },
      "oidc-oauth-route": {
        "ClusterId": "zitadel",
        "Match": {
          "Path": "/oauth/{**catch-all}"
        }
      },
      "oidc-route": {
        "ClusterId": "zitadel",
        "Match": {
          "Path": "/oidc/{**catch-all}"
        }
      },
      "oidc-authorize-route": {
        "ClusterId": "zitadel",
        "Match": {
          "Path": "/authorize"
        }
      },
      "oidc-token-route": {
        "ClusterId": "zitadel",
        "Match": {
          "Path": "/oauth/v2/token"
        }
      },
      "zitadel-ui-route": {
        "ClusterId": "zitadel",
        "Match": {
          "Path": "/ui/{**catch-all}"
        }
      },
      "zitadel-assets-route": {
        "ClusterId": "zitadel",
        "Match": {
          "Path": "/assets/{**catch-all}"
        }
      },
      "zitadel-v2-route": {
        "ClusterId": "zitadel",
        "Match": {
          "Path": "/v2/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "device-manager": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5296"
          }
        }
      },
      "bundle-orchestrator": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5002"
          }
        }
      },
      "telemetry-processor": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5003"
          }
        }
      },
      "identity-manager": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5004"
          }
        }
      },
      "zitadel": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:9080"
          }
        }
      }
    }
  }
}
